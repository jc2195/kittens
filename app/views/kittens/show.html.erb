<%= 'Name: ' %><%= @kitten.name %><br>
<%= 'Age: ' %><%= @kitten.age %><br>
<%= 'Cuteness: ' %><%= @kitten.cuteness %><br>
<%= 'Softness: ' %><%= @kitten.softness %><br>
<%= form_with url: edit_kitten_path(@kitten.id), method: 'get', local: true do |edit_form| %>
        <%= edit_form.submit 'Edit Kitten' %><br>
<% end %>
<%= form_with url: kitten_path(@kitten.id), method: 'delete', local: true do |delete_form| %>
    <%= delete_form.submit 'Delete Kitten' %>
<% end %>